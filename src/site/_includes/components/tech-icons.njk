<nav class="navigation technavigation">
    <ul class="technavigation__menu">

        {% for icon, link in site.tech_links %}

        <li class="technavigation__item">

            <a href="{{ link | safe }}" class="technavigation__link" target="_blank" rel="noopener noreferrer">

                <span class="sr-only">Build with {{ icon }}</span>

                <svg class="icon technavigation__icon" role="img" aria-hidden="true" width="18" height="18">
                    <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/images/icons/icon-library.svg#icon-{{ icon }}"></use>
                </svg>

            </a>

        </li>

        {% endfor %}

    </ul>
</nav>
